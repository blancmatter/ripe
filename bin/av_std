#!/usr/bin/perl



open FILE ">$ARGV[1]";

foreach $line (<FILE>) {
  chomp $line;
  ($id, $s1, $q, $u) = split ("\t", $line)
 
  
  print ($id  $s1 $q  $u);  
  
}
  













############
### SUBS ###
############


sub average{
        my($data) = @_;
        if (not @$data) {
                die("Empty array\n");
        }
        my $total = 0;
        foreach (@$data) {
                $total += $_;
        }
        my $average = $total / @$data;
        return $average;
}
sub stdev{
        my($data) = @_;
        if(@$data == 1){
                return 0;
        }
        my $average = &average($data);
        my $sqtotal = 0;
        foreach(@$data) {
                $sqtotal += ($average-$_) ** 2;
        }
        my $std = ($sqtotal / (@$data-1)) ** 0.5;
        return $std;
}